html 
  outline none
body
  background-color #2a4e7c
  width 100%
  min-width 950px
  overflow auto

header 
  width 100%
  position absolute
  top 0px
  left 0px
  height 50px
  background-color #0e2d54
  border-bottom 3px solid #fff
  min-width 950px
  
  img
    position absolute
    height 60%
    padding 10px
    left 0px

nav
  position absolute
  right 0px
  height 50px
  width 750px
  overflow visible

  ul
    float right
    display inline
    text-align right
    text-decoration none
    padding-right 15px
    margin 14px 0 14px 0
  
  li
    display inline
    color #FFF
    font-family helvetica, sans-serif
    font-style normal
    font-weight 600
    font-size 18px
    padding-left 4px
    padding-right 4px

    a
      display inline
      text-decoration none
      font-family helvetica, sans-serif
      font-style normal
      font-weight 100
      font-size 13px
      color #fff
      letter-spacing 0.25px
    
    a:hover
      color #aaa

#wrapper
  width 900px
  height 600px
  margin 70px auto 0px auto
  padding 0px
  border 2px solid #fff
  background-color #bbb
 	-webkit-box-shadow 0px 5px 10px rgba(25, 25, 25, 1)
  -moz-box-shadow 0px 5px 10px rgba(25, 25, 25, 1)
  box-shadow 0px 5px 10px rgba(25, 25, 25, 1)

h1
  color #0e2d54
  font-family helvetica, sans-serif
  font-style normal
  font-weight 600
  font-size 26px
  text-align center
  letter-spacing 1px
  padding-top 0px
  margin-bottom 0px
  margin-top 8px

.page-description
  color #0e2d54
  font-family helvetica,sans-serif
  font-style normal
  font-weight 100
  font-size 16px
  text-align center
  letter-spacing .5px
  margin-top 1px
  margin-bottom 4px
  padding-left 20px
  padding-right 20px

  div
    display none
    position absolute
    top 70px
    margin 0 auto
    width 900px

  div.current
    display block

.formField
  height 100%
  width 100%

/* FORM STYLING */
form 
  padding-left 15px
  padding-right 15px

  ul 
    padding 0px
    margin 0px

  li 
    text-decoration none
    list-style none

    p 
      clear left
      color #000
      font-family helvetica,sans-serif
      font-style normal
      font-weight 400
      font-size 14px
      text-align left
      letter-spacing 0.5px
      padding 12px 0 3px 1px
      margin 0px
      display block
  
    select 
      clear left
      padding 5px 5px 3px 2px
      outline none
      border-radius 2px

    input, textarea 
      clear left
      padding 3px 5px 3px 5px
      outline none
      border-radius 2px
      text-align middle

    input:focus, textarea:focus 
      border 2px solid #666

  li:first-child 
    padding-top 0px

.submit-button 
  margin-top 10px
  border-radius 6px
  border 2px solid #666
  padding 7px 12px 4px 12px
  background-color #fff
  font-family helvetica,sans-serif
  font-style normal
  font-weight 600
  font-size 14px
  text-align center
  letter-spacing .5px
  color #666

.submit-button:hover
  background-color #aaa
  color #333
  border 2px solid #fff


h2 
  color #0e2d54
  font-family helvetica,sans-serif
  font-style normal
  font-weight 600
  font-size 20px
  text-align center
  letter-spacing 1px
  padding-top 0px
  margin-bottom 2px
  margin-top 8px
  border-bottom 1px solid #0e2d54

.results-box 
  width 300px
  height 490px
  border 1px solid #000
  background-color #fff
  position absolute
  left 20px
  top 80px
  display block
  overflow auto
  padding 10px
  overflow-x hidden

  a 
    text-decoration none
    color #000

  a:hover 
    color #666

.results-text 
  margin 0px
  padding 0px
  text-decoration none
  font-family helvetica,sans-serif
  font-style normal
  font-weight 400
  font-size 13px
  text-align left
  letter-spacing .5px

.txtinput 
   width 500px
   